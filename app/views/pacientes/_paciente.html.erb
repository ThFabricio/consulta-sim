<div id="<%= dom_id paciente %>">
  <p>
    <strong>Nome:</strong>
    <%= paciente.nome %>
  </p>

  <p>
    <strong>Data de Nacimento:</strong>
    <%= paciente.data %>
  </p>

  <p>
    <strong>Cpf:</strong>
    <%= paciente.cpf %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= paciente.email %>
  </p>

  <!-- Endereco -->

  <p>
    <strong>CEP:</strong>
    <%= paciente.endereco.cep %>
  </p>

  <p>
    <strong>Cidade:</strong>
    <%= paciente.endereco.cidade %>
  </p>

  <p>
    <strong>Bairro:</strong>
    <%= paciente.endereco.bairro %>
  </p>

  <p>
    <strong>Logradoro:</strong>
    <%= paciente.endereco.logradoro %>
  </p>

  <p>
    <strong>Complemento:</strong>
    <%= paciente.endereco.complemento %>
  </p>

</div>
